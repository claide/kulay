	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#72767D",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Named entities",
			"scope": "entity.name",
			"settings": {
				"foreground": "#deca82"
			}
		},
		{
			"name": "Function names",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#66FCF1"
			}
		},
		{
			"name": "Tag attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#b2c0f3",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#de9c82"
			}
		},
		{
			"name": "Escape characters",
			"scope": "constant.character.escape",
			"settings": {
