    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "foreground": "#999999",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Named entities",
            "scope": "entity.name",
            "settings": {
                "foreground": "#a5680b"
            }
        },
        {
            "name": "Function names",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#c73d6d"
            }
        },
        {
            "name": "Tag attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#5271bf",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constants",
            "scope": "constant",
            "settings": {
                "foreground": "#946856"
            }
        },
        {
            "name": "Escape characters",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#c73d6d"
            }
        },
        {
            "name": "Inherited classes",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#16865d"
            }
        },
        {
            "name": "CSS ID selectors",
            "scope": "entity.other.attribute-name.id",
            "settings": {
                "foreground": "#c73d6d"
            }
        },
        {
            "name": "Pseudo CSS",
            "scope": [
                "entity.other.attribute-name.pseudo-element",
                "entity.other.attribute-name.pseudo-class"
            ],
            "settings": {
                "foreground": "#16865d"
            }
        },
        {
            "name": "Variable names",
            "scope": [
                "entity.name.variable",
                "variable"
            ],
            "settings": {
                "foreground": "#776BFF"
            }
        },
        {
            "name": "Keywords",
            "scope": "keyword",
            "settings": {
                "foreground": "#b745a0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#505566"
            }
        },
        {
            "name": "Special operators",
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "keyword.operator.delete"
            ],
            "settings": {
                "foreground": "#af48af"
            }
        },
        {
            "name": "Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#776BFF",
                "fontStyle": ""
            }
        },
        {
            "name": "Static types",
            "scope": [
                "entity.name.type",
                "storage.type.cs"
            ],
            "settings": {
                "foreground": "#a5680b"
            }
        },
        {
            "name": "Units",
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#946856"
            }
        },
        {
            "name": "Markup quotes",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#FAB795B3",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup headings",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#776BFF"
            }
        },
        {
            "name": "Markup bold",
            "scope": "markup.bold",
            "settings": {
                "foreground": "#af48af",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Provieded variables",
            "scope": "support.variable",
            "settings": {
                "foreground": "#776BFF"
            }
        },
        {
            "name": "Object/JSON property names",
            "scope": [
                "support.type.property-name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#776BFF"
            }
        },
        {
            "name": "CSS property names",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#505566"
            }
        },
        {
            "name": "Language variables",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#a5680b",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Parameters",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Embedded template punctuation",
            "scope": "string.template meta.embedded",
            "settings": {
                "foreground": "#505566"
            }
        },
        {
            "name": "Markup italics",
            "scope": "markup.italic",
            "settings": {
                "foreground": "#946856E6",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#946856"
            }
        },
        {
            "name": "Markup links",
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#16865d"
            }
        },
        {
            "name": "Storage keywords",
            "scope": "storage",
            "settings": {
                "foreground": "#af48af",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#16865d"
            }
        },
        {
            "name": "Regex",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#946856"
            }
        },
        {
            "name": "Markup link descriptions",
            "scope": "string.other.link",
            "settings": {
                "foreground": "#946856"
            }
        },
        {
            "name": "Embedded section punctuation",
            "scope": "punctuation.section.embedded",
            "settings": {
                "foreground": "#af48af"
            }
        },
        {
            "name": "Markup list punctuation",
            "scope": "punctuation.definition.list",
            "settings": {
                "foreground": "#946856"
            }
        },
        {
            "name": "Provided (support) values",
            "scope": "support",
            "settings": {
                "foreground": "#a5680b"
            }
        },
        {
            "name": "Provided functions",
            "scope": "support.function",
            "settings": {
                "foreground": "#c73d6d"
            }
        },
        {
            "name": "Tag brackets",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#505566",
                "fontStyle": ""
            }
        },
        {
            "name": "Key/value separators",
            "scope": "punctuation.separator",
            "settings": {
                "foreground": "#505566"
            }
        },
        {
            "name": "Template expressions",
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "#af48af"
            }
        },
    ]
}